@model IEnumerable<Microsoft.AspNetCore.Identity.IdentityRole>

<h1>Roles</h1>
<a asp-action="CreateRole">Create</a>

<table class="table">

    <thead>
        <td>Id</td>
        <td>Name</td>
    </thead>
    <tbody>
        @foreach (var role in Model)
        {
        <tr>
            <td>role.Id</td>
            <td>role.Name</td>
        </tr>
        }
    </tbody>
</table>