@model IEnumerable<Microsoft.AspNetCore.Identity.IdentityRole>

<h1>Roles</h1>
<a asp-controller="Role" asp-action="Create">Create</a>

<table class="table">
    <tr>
        <th>Id</th>
        <th>Name</th>
    </tr>
    <tbody>
        @foreach (var role in Model)
        {
        <tr>
            <td>role.Id</td>
            <td>role.Name</td>
        </tr>
        }
    </tbody>
</table>